@model List<BasicEmployeeWebApp.Models.EmployeeType>

@{
    ViewBag.Title = "Index";
}
<h2>Index</h2>
<!--class="table table-striped table-condensed"-->
<table class="table table-striped table-condensed">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { employeeTypeId = @item.Id })
                @Html.ActionLink("Delete", "Delete", new { employeeTypeId = @item.Id })
            </td>
        </tr>
        }
    </tbody>
</table>
@Html.ActionLink("Make new EmployeeType", "Create", null, new { @class = "btn btn-success" })
